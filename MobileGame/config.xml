<?xml version='1.0' encoding='utf-8'?>
<widget id="com.shortbreadcoast.wordrockets" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>WordRockets</name>
    <description>
        Wowowow, wowowow.
    </description>
    <author email="bugsby.carlin@gmail.com" href="http://shortbreadcoast.com">
        Mattsby
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="SplashScreen" value="none"/>
        <preference name="BackupWebStorage" value="local"/>
        <preference name="DisallowOverscroll" value="true" />
        <custom-config-file parent="UIStatusBarHidden" platform="ios" target="*-Info.plist">
            <true/>
        </custom-config-file>
        <custom-config-file parent="UIViewControllerBasedStatusBarAppearance" platform="ios" target="*-Info.plist">
            <false/>
        </custom-config-file>
        <!-- <splash src="icons/splash_screen.png" width="768" height="1024"/> -->
    </platform>
     <!-- <icon src="icons/icon.png" /> -->
</widget>
